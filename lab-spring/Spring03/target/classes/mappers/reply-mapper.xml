<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="edu.spring.ex03.mappers.ReplyMapper">

    <select id="selectAll" resultType="Reply">
    SELECT * FROM test_reply ORDER BY rno DESC
    </select>
    
    <delete id="delete">
    DELETE FROM test_reply WHERE rno = #{rno}
    </delete>

</mapper>








